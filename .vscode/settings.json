{
  "i18n-ally.localesPaths": ["public/locales", "src/shared/config/i18n"],
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": true
  },
  "scss.validate": false,
  "stylelint.snippet": [
    "scss",
  ],
  "stylelint.validate": [
    "scss",
  ],
  "i18n-ally.keystyle": "nested"
}
